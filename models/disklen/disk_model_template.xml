<mujoco model="disk model">
    <option gravity="0 0 -9.81" />

    <!-- This is our texture -->
    <asset>
        <material name="white" rgba="1 1 1 1" />
    </asset>

    <worldbody>
        <!-- settings -->
        <camera name="cam0" pos="0 -1.8 -5" euler="90 0 0" mode="trackcom" />

        <!-- lower zylinder -->
        <body name="lower zylinder" pos="0 0 .2" euler="0 0 0">

            <geom type="cylinder" size=".05 .2" rgba="0 0 1 0.5" />
            <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />

            <!-- lower plate -->
            <body name="lower plate" pos="0 0 .2" euler="0 0 0">
                <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />
                <geom type="box" size=".2 .2 .02" rgba="0 0 1 1" />

                <site name="tendon1_start" pos="0.2 0.2 0" type="sphere" size="0.01" />
                <site name="tendon2_start" pos="-0.2 -0.2 0" type="sphere" size="0.01" />
                <site name="tendon3_start" pos=" 0.2 -0.2 0" type="sphere" size="0.01" />
                <site name="tendon4_start" pos="-0.2  0.2 0" type="sphere" size="0.01" />

                <!-- lower middle zylinder -->
                <body name="lower middle zylinder" pos="0 0 .05" euler="0 0 0">
                    <geom type="cylinder" size=".02 .05" rgba="0 0 1 0.5" />
                    <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />

                    <!-- center ball -->
                    <body name="center ball" pos="0 0 .07" euler="0 0 0">

                        <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />
                        <geom type="sphere" size=".02 " rgba="1 0 0 .4" />
                        <joint type="ball" pos="0 0 0" />

                        <!-- upper middle zylinder -->
                        <body name="upper middle zylinder" pos="0 0 .07" quat="1 0 0 0">


                            <geom type="cylinder" size=".02 .05" rgba="0 0 1 0.5" />
                            <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />

                            <!-- upper plate -->
                            <body name="upper plate" pos="0 0 .05" euler="0 0 0">
                                <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />
                                <geom type="box" size=".2 .2 .02" rgba="0 0 1 1" />


                                <site name="tendon1_end" pos="0.2 0.2 0" type="sphere" size="0.01" />
                                <site name="tendon2_end" pos="-0.2 -0.2 0" type="sphere" size="0.01" />
                                <site name="tendon3_end" pos="0.2 -0.2 0" type="sphere" size="0.01" />
                                <site name="tendon4_end" pos="-0.2 0.2 0" type="sphere" size="0.01" />

                                <!-- upper zylinder -->
                                <body name="upper zylinder" pos="0 0 .2" euler="0 0 0">
                                    <geom type="cylinder" size=".05 .2" rgba="0 0 1 0.5" />
                                    <inertial pos="0 0 0" mass="1" diaginertia=".01 .01 .01" />
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <!-- tendons -->
    <tendon>
        <spatial name="tendon1" stiffness="{{tendon1.stiffness}}" damping="{{tendon1.damping}}"
            springlength="{{tendon1.springlength}}" width="0.002" rgba="0.9 0.2 0.2 0.9">
            <site site="tendon1_start" />
            <site site="tendon1_end" />
        </spatial>


        <spatial name="tendon2" stiffness="{{tendon2.stiffness}}" damping="{{tendon2.damping}}"
            springlength="{{tendon2.springlength}}" width="0.002" rgba="0.9 0.2 0.2 0.9">
            <site site="tendon2_start" />
            <site site="tendon2_end" />
        </spatial>

        <spatial name="tendon3" stiffness="{{tendon3.stiffness}}" damping="{{tendon3.damping}}"
            springlength="{{tendon3.springlength}}" width="0.002" rgba="0.9 0.2 0.2 0.9">
            <site site="tendon3_start" />
            <site site="tendon3_end" />
        </spatial>

        <spatial name="tendon4" stiffness="{{tendon4.stiffness}}" damping="{{tendon4.damping}}"
            springlength="{{tendon4.springlength}}" width="0.002" rgba="0.9 0.2 0.2 0.9">
            <site site="tendon4_start" />
            <site site="tendon4_end" />
        </spatial>

    </tendon>

</mujoco>